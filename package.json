{
  "name": "rest-tool-common",
  "description": "A tool to document emulate and test RESTful APIs.",
  "version": "2.0.2",
  "keywords": [
    "rest",
    "restful",
    "api",
    "docs",
    "util",
    "tool",
    "utility"
  ],
  "author": {
    "name": "Tam√°s Benke",
    "email": "tombenke@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/tombenke/rest-tool-common.git"
  },
  "bin": {},
  "main": "dist/index.js",
  "dependencies": {
    "datafile": "^3.1.2",
    "handlebars": "^4.0.11",
    "lodash": "^4.17.4",
    "marked": "^0.3.7",
    "wrench": "1.5.6"
  },
  "devDependencies": {
    "pre-push": "^0.1.1",
    "babel-cli": "6.16.0",
    "babel-core": "6.17.0",
    "babel-loader": "6.2.5",
    "babel-plugin-transform-object-rest-spread": "6.20.2",
    "babel-polyfill": "6.16.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "6.16.0",
    "babel-register": "6.16.3",
    "eslint": "^3.13.1",
    "jsdoc": "^3.5.5",
    "coveralls": "^2.11.15",
    "cross-env": "^2.x.x",
    "istanbul": "1.0.0-alpha.2",
    "mocha": "^3.5.3",
    "mocha-lcov-reporter": "^1.2.0",
    "should": "3.1.2",
    "expect": "^1.6.0"
  },
  "scripts": {
    "build": "babel src --presets babel-preset-es2015 --out-dir dist",
    "lint": "eslint src/**.js",
    "test": "cross-env NODE_ENV=test mocha --recursive --compilers js:babel-register 'src/**/*.spec.js'",
    "test:watch": "npm test -- --watch",
    "coverage": "NODE_ENV=test istanbul cover _mocha -- recursive --compilers js:babel-register --timeout 15000 'src/**/*.spec.js'",
    "coveralls": "NODE_ENV=test istanbul cover _mocha --report lcovonly -- recursive --compilers js:babel-register --timeout 15000 'src/**/*.spec.js' -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "docs": "rm -fr ./docs/* && jsdoc -c ./.jsdoc.json",
    "prepublish": "npm run lint && npm run test && npm run build && npm run docs",
    "all": "npm run lint && npm run coverage && npm run build && npm run docs"
  }
}
